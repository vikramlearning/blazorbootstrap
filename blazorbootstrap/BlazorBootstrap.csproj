<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>

		<PackageId>Blazor.Bootstrap</PackageId>
		<Version>0.0.1</Version>
		<PackageVersion>0.0.1</PackageVersion>

		<!--<PackageIconUrl></PackageIconUrl>-->
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/gvreddy04/blazorbootstrap</PackageProjectUrl>
		<RepositoryUrl>https://github.com/gvreddy04/blazorbootstrap</RepositoryUrl>
		<PackageTags>Blazor, Blazor WebAssembly, Bootstrap, Bootstrap 5.x</PackageTags>		
		<Description>BlazorBootstrap is a component library built on top of Blazor and CSS framework Bootstrap.</Description>
		<Authors>Vikram</Authors>
		<!--<Company>your_company</Company>-->
		<Copyright>Copyright 2021 Vikram</Copyright>
		
		<TargetFramework>net5.0</TargetFramework>
		<OutputType>Library</OutputType>
		<IsPackable>true</IsPackable>
		<LangVersion>10.0</LangVersion>

	</PropertyGroup>

	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.9" />
	</ItemGroup>

</Project>
